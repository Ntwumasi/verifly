    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.4",
    "stripe": "^16.1.0",
    "nodemailer": "^6.9.14",
    "twilio": "^5.2.2",
    "axios": "^1.7.2",
    "bull": "^4.15.1",
    "redis": "^4.6.14",
