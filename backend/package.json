{
  "name": "verifly-backend",
  "version": "1.0.0",
  "description": "Verifly API backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
